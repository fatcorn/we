<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.den.we.admin.mapper.SysAdminRoleMapper">
    <select id="findRolesByAdminId" resultType="com.den.we.entity.SysRole" parameterType="java.lang.Long">
        select sr.* from sys_admin_role sar
            join sys_role sr on sar.role_id=sr.id
        where sar.admin_id=#{adminId}
    </select>
</mapper>
